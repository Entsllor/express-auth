{
  "name": "just-auth",
  "version": "0.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run ./src/index.ts",
    "run-migrations-and-start": "bun migration:run && bun start",
    "style": "bun prettier ./src -w --log-level=warn",
    "migration:create": "typeorm-ts-node-esm migration:create",
    "migration:generate": "typeorm-ts-node-esm migration:generate -d src/core/data-source.ts",
    "migration:run": "typeorm-ts-node-esm migration:run -d src/core/data-source.ts",
    "migration:revert": "typeorm-ts-node-esm migration:revert -d src/core/data-source.ts"
  },
  "dependencies": {
    "@vinejs/vine": "^1.6.0",
    "backend-batteries": "^0.2.0",
    "body-parser": "^1.19.1",
    "chalk": "^5.3.0",
    "cookie-parser": "~1.4.4",
    "date-fns": "^2.30.0",
    "express": "^4.17.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "on-finished": "^2.4.1",
    "pg": "^8.4.0",
    "radash": "^11.0.0",
    "reflect-metadata": "^0.1.13",
    "supertest": "^6.3.3",
    "typeorm": "0.3.17",
    "typescript": "^5.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.9.1",
    "@types/on-finished": "^2.3.4",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.2",
    "bun-types": "^1.0.16",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
